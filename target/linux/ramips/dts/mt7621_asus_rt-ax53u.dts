&nand {
	mediatek,nmbm;
	compatible = "mediatek,nmbm";
	#address-cells = <1>;
	#size-cells = <1>;
	status = "okay";

	flash@0 {
		mediatek,nmbm;
		compatible = "mediatek,nmbm";
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0>;
/*		nand-ecc-mode = "hw";
*/		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;
		partition@0 {
			label = "u-boot";
			reg = <0x0 0xe0000>;
			read-only;
		};

		partition@e0000 {
			label = "u-boot-env";
			reg = <0xe0000 0x100000>;
			read-only;
		};

		factory: partition@1e0000 {
			label = "factory";
			reg = <0x1e0000 0x100000>;
			read-only;
		};

		factory2: partition@2e0000 {
			label = "factory2";
			reg = <0x2e0000 0x100000>;
			read-only;
		};

		partition@3e0000 {
			compatible = "denx,fit";
			label = "kernel";
			reg = <0x3e0000 0x3200000>;
		};

/*		partition@3e0000 {
			label = "firmware";
			reg = <0x3e0000 0x400000>;
		};

		partition@7e0000 {
			label = "rootfs";
			reg = <0x7e0000 0x2e00000>;
		};
*/
		partition@35e0000 {
			label = "kernel2";
			reg = <0x35e0000 0x3200000>;
		};

		partition@67e0000 {
			label = "jffs2";
			reg = <0x67e0000 0x1020000>;
		};

		/*
		 * Leave 512 KiB for the bad block table
		 */
		};
	};
};

&pcie {
	status = "okay";
};

&pcie1 {
	wifi@0,0 {
		compatible = "mediatek,mt76";
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&factory 0x0>;
	};
};

&gmac0 {
	nvmem-cells = <&macaddr_factory_4>;
	nvmem-cell-names = "mac-address";
};

&switch0 {
	ports {
		port@0 {
			status = "okay";
			label = "lan1";
		};

		port@1 {
			status = "okay";
			label = "lan2";
		};

		port@2 {
			status = "okay";
			label = "lan3";
		};

		port@4 {
			status = "okay";
			label = "wan";
			nvmem-cells = <&macaddr_factory_a>;
			nvmem-cell-names = "mac-address";
		};
	};
};

