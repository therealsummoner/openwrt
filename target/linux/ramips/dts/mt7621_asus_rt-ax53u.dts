&factory {
	compatible = "nvmem-cells";
	#address-cells = <1>;
	#size-cells = <1>;

	macaddr_factory_4: macaddr@4 {
		reg = <0x4 0x6>;
	};

	macaddr_factory_a: macaddr@a {
		reg = <0xa 0x6>;
	};
};

&uartlite2 {
	status = "okay";
};

&uartlite3 {
	status = "okay";
};
